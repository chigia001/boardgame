{
  "extends": [
    "plugin:lodash-fp/recommended",
    "prettier-standard"
  ],
  "plugins": [
    "promise",
    "lodash-fp",
    "prettier",
    "standard"
  ],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": true
    }
  },
  "rules": {
    "prettier/prettier": "error"
  }
}